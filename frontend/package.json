{
  "name": "deplio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "doppler run -- vite dev",
    "build": "vite build",
    "preview": "doppler run -- vite preview",
    "test": "doppler run -- playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "doppler run -- prettier --plugin-search-dir . --check . && eslint .",
    "format": "doppler run -- prettier --plugin-search-dir . --write ."
