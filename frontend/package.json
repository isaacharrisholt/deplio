    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.38.4",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.24.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^20.11.24",
